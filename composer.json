{
    "name": "jscustom/laravel-image-resize",
    "description": "Laravel package for resizing images. Supports PNG, JPG, JPEG, GIF.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0 | ^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 | ^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JSCustom\\ImageResize\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jediael Saliba",
            "email": "developer.jeddsaliba@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "extra": {
        "laravel": {
            "providers": [
                "JSCustom\\ImageResize\\Providers\\ImageResizeServiceProvider"
            ],
            "aliases": {
                "Qbo": "JSCustom\\ImageResize\\Facade"
            }
        }
    }
}
